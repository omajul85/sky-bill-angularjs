{
  "name": "sky-bill",
  "version": "0.1.0",
  "description": "App that displays a customer's Sky bill",
  "scripts": {
    "start-app": "cd app && ../node_modules/http-server/bin/http-server",
    "start-selenium": "./node_modules/protractor/bin/webdriver-manager start",
    "install-selenium-dependencies": "./node_modules/protractor/bin/webdriver-manager update",
    "install-bower-dependencies": "./node_modules/bower/bin/bower install",
    "test-features": "./node_modules/protractor/bin/protractor test/protractor.conf.js",
    "test-units": "./node_modules/karma-cli/bin/karma start test/karma.conf.js",
    "postinstall": "bower install"
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omajul85/sky-bill"
  },
  "dependencies": {
    "bower": "^1.7.9",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-cli": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "protractor": "^3.3.0",
    "protractor-http-mock": "^0.7.0"
  }
}
